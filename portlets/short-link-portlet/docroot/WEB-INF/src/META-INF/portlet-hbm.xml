<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.shortlink.model.ShortLinkEntry" />
	<class name="com.liferay.shortlink.model.impl.ShortLinkEntryImpl" table="ShortLink_ShortLinkEntry">
		<cache usage="read-write" />
		<id name="shortLinkEntryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="originalURL" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="shortURL" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="autogenerated" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="active" type="com.liferay.portal.dao.orm.hibernate.BooleanType" column="active_" />
	</class>
</hibernate-mapping>