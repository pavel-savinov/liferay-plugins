<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.consumer.manager.model.Consumer" />
	<import class="com.liferay.consumer.manager.model.ConsumerExtensionInstance" />
	<class name="com.liferay.consumer.manager.model.impl.ConsumerImpl" table="CM_Consumer">
		<cache usage="read-write" />
		<id name="consumerId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="consumerKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.liferay.consumer.manager.model.impl.ConsumerExtensionInstanceImpl" table="CM_ConsumerExtensionInstance">
		<cache usage="read-write" />
		<id name="consumerExtensionInstanceId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="consumerExtensionKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="consumerId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="typeSettings" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>