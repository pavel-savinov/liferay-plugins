<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="tika-web" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<dependencies defaultconf="default">
		<dependency name="com.xuggle.xuggler" org="com.liferay" rev="5.4">
			<artifact ext="jar" m2:classifier="noarch" name="com.xuggle.xuggler" />
		</dependency>
		<dependency name="commons-lang" org="commons-lang" rev="2.6" />
		<dependency name="tika-core" org="org.apache.tika" rev="1.6" />
		<dependency name="tika-parsers" org="org.apache.tika" rev="1.6" />
	</dependencies>
</ivy-module>